@{
   ViewBag.Title = "Conger's Heating & Cooling, Inc | Oil Delivery & HVAC Installation | 978.728.4757";
}
<div class="row">
    <div class="large-12 columns">
        <div class="panel transparent">
            <h1>Delivery Area</h1>
            <div class="row">
                <div class="large-12 medium-12 small-12 columns text-center">
                    <iframe src="https://www.google.com/maps/d/embed?mid=zCKxpGgVuppA.kjyzCEnxpvu4" width="100%" height="480"></iframe>
                    <h6>* The map above is approximate.</h6s>
                </div>
            </div>
            <div class="panel radius">
                <div class="row">
                    <div class="large-12 medium-12 small-12 columns text-center">
                        <div class="flow">
                            <ul>
                                @foreach ( string town in Model.ServiceAreas ) {
                                <li>@town</li>}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            @Html.Partial( "_Footer" )
        </div>
    </div>

</div>
